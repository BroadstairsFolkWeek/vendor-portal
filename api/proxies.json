{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "redirectcode": {
      "matchCondition": {
        "methods": ["GET"],
        "route": "/api/redirect"
      },
      "backendUri": "https://localhost/api/redirect",
      "requestOverrides": {
        "backend.request.querystring.code": "",
        "backend.request.querystring.authcode": "{request.querystring.code}"
      }
    }
  }
}
